<!ELEMENT cwonder_config (settings,renderpolygon) >

<!ELEMENT settings EMPTY>
<!ATTLIST settings
    projectpath	   CDATA  #REQUIRED
    maxNoSources   CDATA  #REQUIRED
>

<!ELEMENT renderpolygon (point*,elevation) >
<!ATTLIST renderpolygon
    roomname    CDATA  #REQUIRED
>

<!ELEMENT point EMPTY>
<!ATTLIST point
    x	CDATA  #REQUIRED
    y	CDATA  #REQUIRED
    z	CDATA  #REQUIRED
>

<!ELEMENT elevation EMPTY>
<!ATTLIST elevation
    y1	CDATA  #REQUIRED
    z1	CDATA  #REQUIRED
    y2	CDATA  #REQUIRED
    z2	CDATA  #REQUIRED
>

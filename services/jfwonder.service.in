[Unit]
Description=JFWonder
After=network-online.target jack.service
Requires=jack.service


[Install]
WantedBy=default.target

[Service]
Type=simple
PrivateTmp=true
# EnvironmentFile=/usr/local/etc/wonder/cluster.conf
ExecStart=@bin_dir@/jfwonder
LimitRTPRIO=95
LimitRTTIME=infinity
LimitMEMLOCK=infinity
